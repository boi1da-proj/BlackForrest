<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <handlers>
      <add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModuleV2" resourceType="Unspecified" />
    </handlers>
    <aspNetCore
      processPath="C:\apps\rhino.compute\current\rhino.compute.exe"
      arguments="--port 80"
      stdoutLogEnabled="true"
      stdoutLogFile="C:\logs\rhino-compute\stdout"
      forwardWindowsAuthToken="true"
      hostingModel="OutOfProcess" />
  </system.webServer>
</configuration>

